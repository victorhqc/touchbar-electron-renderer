{
  "name": "touchbar-electron-renderer",
  "private": false,
  "version": "0.3.0",
  "description": "A React renderer for building declarative touchbar interfaces for Electron in Mac OSX.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "compile": "rollup -c",
    "compile:watch": "rollup -c --watch",
    "release": "npm run compile && np"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/victorhqc/touchbar-electron-renderer.git"
  },
  "keywords": [
    "react",
    "react-renderer",
    "touchbar",
    "macosx"
  ],
  "files": [
    "dist"
  ],
  "author": "Victor Quiroz Castro <victorhqc@gmail.com>",
  "license": "MIT",
  "contributors": [
    "Victor Quiroz Castro <victorhqc@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/victorhqc/touchbar-electron-renderer/issues"
  },
  "homepage": "https://github.com/victorhqc/touchbar-electron-renderer#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "electron": "^4.0.4",
    "electron-rebuild": "^1.8.4",
    "eslint": "^5.13.0",
    "eslint-config-healthier": "^1.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "lodash": "^4.17.11",
    "np": "^4.0.2",
    "prettier": "^1.16.4",
    "react": "^16.8.2",
    "rollup": "^1.16.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.0"
  },
  "peerDependencies": {
    "electron": "^4.0.4",
    "lodash": "^4.17.11",
    "react": "^16.8.2"
  },
  "dependencies": {
    "react-reconciler": "^0.20.2",
    "uuid": "^3.3.2"
